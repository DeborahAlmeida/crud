                <a href="deletar/{{id}}" class="deletar">
                    <img src="../img/trash.png" style="width=20px; height:20px;" alt="">
                </a>
            </td>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{age}}</td>
        </tr>
    {{/data}}
</table>