<table>
    <tr>
        <td>Action</td>
        <td>Id</td>
        <td>Name</td>
        <td>Age</td>
    </tr>
    {{#data}}
        <tr>
            <td>
                <a href="update/{{id}}" class="update">
                    <img src="../img/edit.png" style="width=18px; height:18px;" alt="">
